    "@angular/compiler-cli": "^21.0.0",
    "@types/sockjs-client": "^1.5.4",
    "@types/stompjs": "^2.3.10",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
